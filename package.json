{
  "name": "@rmaher001/scrypted-llm-notifier",
  "version": "0.1.3",
  "description": "Enhance notifications with LLM-generated descriptions of detection images",
  "author": "rmaher001",
  "repository": {
    "type": "git",
    "url": "https://github.com/rmaher001/llm-notifier.git"
  },
  "bugs": {
    "url": "https://github.com/rmaher001/llm-notifier/issues"
  },
  "homepage": "https://github.com/rmaher001/llm-notifier#readme",
  "main": "out/main.nodejs.js",
  "scripts": {
    "scrypted-setup-project": "scrypted-setup-project",
    "prescrypted-setup-project": "scrypted-package-json",
    "build": "node node_modules/@scrypted/sdk/bin/scrypted-webpack.js && mkdir -p dist && cp out/plugin.zip dist/plugin.zip",
    "prescrypted-vscode-launch": "scrypted-webpack",
    "scrypted-vscode-launch": "scrypted-deploy-debug",
    "scrypted-deploy-debug": "scrypted-deploy-debug",
    "scrypted-debug": "scrypted-debug",
    "scrypted-deploy": "scrypted-deploy",
    "scrypted-changelog": "scrypted-changelog",
    "scrypted-package-json": "scrypted-package-json",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "scrypted",
    "plugin",
    "notifier",
    "llm",
    "ai"
  ],
  "scrypted": {
    "name": "LLM Notifier",
    "type": "Provider",
    "interfaces": [
      "MixinProvider",
      "Settings"
    ]
  },
  "devDependencies": {
    "@scrypted/client": "^1.3.18",
    "@scrypted/common": "^1.0.2",
    "@scrypted/sdk": "^0.5.44",
    "@types/node": "^22.1.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "jpeg-js": "^0.4.4"
  }
}
